{
    "final_space": true,
    "console_title": true,
    "console_title_style": "folder",
    "blocks": [
        {
            "type": "prompt",
            "alignment": "left",
            "horizontal_offset": 0,
            "vertical_offset": 0,
            "segments": [
                {
                    "type": "text",
                    "style": "plain",
                    "foreground": "#333",
                    "properties": {
                        "prefix": "",
                        "text": "\uE0Ba"
                    }
                },
                {
                    "type": "path",
                    "style": "diamond",
                    "powerline_symbol": "",
                    "invert_powerline": false,
                    "foreground": "#ddd",
                    "background": "#1976d2",
                    "trailing_diamond": "",
                    "properties": {
                        "prefix": "  ",
                        "style": "full"
                    }
                }
            ]
        },
        {
            "type": "prompt",
            "alignment": "right",
            "segments": [
                {
                    "type": "time",
                    "style": "plain",
                    "foreground": "#1976d2",
                    "properties": {
                        "time_format": "15:04:05"
                    }
                },
                {
                    "type": "executiontime",
                    "style": "powerline",
                    "powerline_symbol": "\uE0B0",
                    "foreground": "#ddd",
                    "background": "#8800dd",
                    "properties": {
                        "threshold": 500,
                        "style": "austin",
                        "prefix": " <#fefefe>\ufbab</> "
                    }
                }
            ]
        },
        {
            "type": "newline"
        },
        {
            "type": "prompt",
            "alignment": "left",
            "horizontal_offset": 0,
            "vertical_offset": 0,
            "segments": [
                {
                    "type": "text",
                    "style": "plain",
                    "foreground": "#333",
                    "properties": {
                        "prefix": "",
                        "text": "\uE0Be"
                    }
                },
                {
                    "type": "git",
                    "style": "powerline",
                    "powerline_symbol": "",
                    "invert_powerline": false,
                    "foreground": "#193549",
                    "background": "#43aa7f",
                    "leading_diamond": "",
                    "trailing_diamond": "",
                    "properties": {
                        "display_stash_count": true,
                        "display_upstream_icon": true
                    }
                },
                {
                    "type": "dotnet",
                    "style": "powerline",
                    "powerline_symbol": "",
                    "invert_powerline": false,
                    "foreground": "#ddd",
                    "background": "#6a1b9a",
                    "leading_diamond": "",
                    "trailing_diamond": "",
                    "properties": {
                        "display_version": true,
                        "prefix": "  "
                    }
                },
                {
                    "type": "node",
                    "style": "powerline",
                    "powerline_symbol": "",
                    "invert_powerline": false,
                    "foreground": "#ddd",
                    "foreground_templates": null,
                    "background": "#6CA35E",
                    "background_templates": null,
                    "leading_diamond": "",
                    "trailing_diamond": "",
                    "properties": {
                        "prefix": " "
                    }
                },
                {
                    "type": "angular",
                    "style": "powerline",
                    "powerline_symbol": "",
                    "foreground": "#000000",
                    "background": "#c2185b",
                    "properties": {
                        "prefix": "\uE753"
                    }
                },
                {
                    "type": "java",
                    "style": "powerline",
                    "powerline_symbol": "\uE0B0",
                    "foreground": "#ddd",
                    "background": "#4063D8",
                    "properties": {
                        "prefix": " \uE738 "
                    }
                },
                {
                    "type": "python",
                    "style": "powerline",
                    "powerline_symbol": "\uE0B0",
                    "foreground": "#100e23",
                    "background": "#906cff",
                    "properties": {
                        "prefix": " \uE235 "
                    }
                },
                {
                    "type": "root",
                    "style": "powerline",
                    "powerline_symbol": "",
                    "invert_powerline": false,
                    "foreground": "#ddd",
                    "background": "#dddf66",
                    "leading_diamond": "",
                    "trailing_diamond": "",
                    "properties": null
                },
                {
                    "type": "exit",
                    "style": "powerline",
                    "powerline_symbol": "",
                    "invert_powerline": false,
                    "foreground": "#ddd",
                    "background": "#1976d2",
                    "leading_diamond": "",
                    "trailing_diamond": "",
                    "properties": {
                        "always_enabled": true,
                        "color_background": true,
                        "display_exit_code": false,
                        "error_color": "#f1184c",
                        "prefix": " "
                    }
                }
            ]
        }
    ]
}